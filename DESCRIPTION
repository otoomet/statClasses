	'setAs-Stat.R'
	'setAs-lm.R'
	'stdEr-Estimates.R'
	'stdEr-Stat.R'
	'stdEr-lm.R'
	'show-Estimates.R'
	'show-coefTable.R'
	'statClasses.R'
	'xtable-CoefTable.R'
